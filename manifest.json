{
   "background": {
      "persistent": false,
   },
   "chrome_url_overrides": {
      "newtab": "mortality.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Replaces New Tab with your precise age; as well as a visual representation of months you've lived.",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
   "manifest_version": 2,
   "name": "Mortality",
   "offline_enabled": true,
   "version": "2.2"
}
